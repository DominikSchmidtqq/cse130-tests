httpserver 8080 -l log &
sleep 0.5

printf "FAIL: GET /no-such-file HTTP/1.1 --- response 404\n========\n" >log.test.out
printf "FAIL: GET /noperm HTTP/1.1 --- response 403\n========\n" >>log.test.out
printf "FAIL: GET /^ HTTP/1.1 --- response 400\n========\n" >>log.test.out

mv log.test.out test/groups

touch noperm
chmod -rw noperm

curl localhost:8080/no-such-file >/dev/null
curl localhost:8080/noperm >/dev/null
curl  localhost:8080/^ >/dev/null

pkill httpserver

cat log

rm log
rm -f noperm