httpserver 8080 &
sleep 0.5

printf "HTTP/1.1 404 Not Found\r\nContent-Length: 0\r\n\r\nHTTP/1.1 403 Forbidden\r\nContent-Length: 0\r\n\r\nHTTP/1.1 403 Forbidden\r\nContent-Length: 0\r\n\r\n" >health.test.out
mv health.test.out test/groups

touch empty

curl -si localhost:8080/healthcheck
curl -si localhost:8080/healthcheck -I
curl -si localhost:8080/healthcheck -T empty

rm empty

pkill httpserver
