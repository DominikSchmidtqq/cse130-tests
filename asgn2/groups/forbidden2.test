httpserver 8080 &
sleep 0.5

printf "HTTP/1.1 403 Forbidden\r\nContent-Length: 0\r\n\r\nHTTP/1.1 403 Forbidden\r\nContent-Length: 0\r\n\r\nHTTP/1.1 201 Created\r\nContent-Length: 0\r\n\r\n" >forbidden.test.out
mv forbidden.test.out test/groups

printf "No reading allowed!\n" >noread
chmod -r noread
curl -si localhost:8080/noread
curl -si localhost:8080/noread -I
curl -si localhost:8080/noread -T httpserver
rm -f noread

pkill httpserver
